{
    "name": "@statsanytime/trade-bots",
    "description": "Simple framework for building CS trade-bots!",
    "version": "1.0.0-alpha.1",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "bin": {
        "stattb": "./dist/cli.mjs"
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "unbuild",
        "build-stub": "unbuild --stub",
        "stattb": "node dist/stattb.mjs",
        "test": "vitest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/statsanytime/trade-bots.git"
    },
    "homepage": "https://github.com/statsanytime/trade-bots",
    "author": "Pixel",
    "license": "MIT",
    "dependencies": {
        "big.js": "^6.2.1",
        "citty": "^0.1.4",
        "consola": "^3.2.3",
        "dayjs": "^1.11.10",
        "hookable": "^5.5.3",
        "unctx": "^2.3.1",
        "unstorage": "^1.9.0"
    },
    "devDependencies": {
        "@statsanytime/trade-bots-csgoempire": "^1.0.0-alpha.1",
        "@statsanytime/trade-bots-csgofloat": "^1.0.0-alpha.1",
        "@statsanytime/trade-bots-pricempire": "^1.0.0-alpha.1",
        "@statsanytime/trade-bots-steam": "^1.0.0-alpha.1",
        "@types/big.js": "^6.2.1",
        "@types/node": "^20.6.2",
        "csgoempire-wrapper": "^0.1.19",
        "msw": "^1.3.1",
        "unbuild": "^2.0.0",
        "vitest": "^0.34.5"
    }
}
